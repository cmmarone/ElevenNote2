@model System.Collections.Generic.ICollection<ElevenNote.Data.Note>

<span>
    @if (Model.Count > 0)
    {
        foreach (var note in Model)
        {
            <table>
                <tr>
                    <td>@note.Title - @note.CreatedUtc.LocalDateTime.ToString("G")</td>
                </tr>
            </table>
        }
    }
</span>

